<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">

    <Label text="CHESS">
      <font>
         <Font name="System Bold" size="35.0" />
      </font>
      <VBox.margin>
         <Insets left="250.0" top="50.0" />
      </VBox.margin></Label>
    <Button fx:id="singleplayerBtn" onAction="#startSingleplayer" text="Singleplayer">
      <font>
         <Font size="20.0" />
      </font>
      <VBox.margin>
         <Insets left="240.0" top="50.0" />
      </VBox.margin>
      <padding>
         <Insets left="13.0" right="13.0" />
      </padding></Button>
   <Button fx:id="multiplayerBtn" mnemonicParsing="false" onAction="#startMultiplayer" text="Multiplayer">
      <font>
         <Font size="20.0" />
      </font>
      <VBox.margin>
         <Insets left="242.0" top="20.0" />
      </VBox.margin>
      <padding>
         <Insets left="18.0" right="18.0" />
      </padding>
   </Button>
   <Button fx:id="settingsBtn" mnemonicParsing="false" onAction="#openSettings" text="Settings">
      <font>
         <Font size="20.0" />
      </font>
      <VBox.margin>
         <Insets left="241.0" top="20.0" />
      </VBox.margin>
      <padding>
         <Insets left="30.0" right="30.0" />
      </padding>
   </Button>
   <Button fx:id="quitBtn" mnemonicParsing="false" onAction="#quitGame" text="Quit">
      <font>
         <Font size="20.0" />
      </font>
      <VBox.margin>
         <Insets left="241.0" top="20.0" />
      </VBox.margin>
      <padding>
         <Insets left="47.0" right="47.0" />
      </padding>
   </Button>

</VBox>
